<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type='org.jorgecardoso.purewidgets.demo.placeinteraction.client.ui.application.ApplicationListUi.Style'>
	.main {
		margin-left:auto;
		margin-right:auto;
		border: 1px solid blue;
	}
	.applications {
		margin-left:auto;
		margin-right:auto;
		border: 1px solid black;
		text-align:center;
	}
	.list {
			font-size:1em;
			border:none;
			overflow:auto;
			width:100%;
			text-align:center;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.main}">
		<div class="title">Applications in <b><g:InlineLabel  ui:field="placeName"/></b></div> 
		<g:HTMLPanel styleName="{style.applications}" ui:field="panel"/>
	</g:HTMLPanel>
</ui:UiBinder> 