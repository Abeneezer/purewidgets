<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.inline {
			display:inline;
		}
	</ui:style>
	<g:HTMLPanel>
		<div class="title"><b>Interact</b> with applications in <b><g:Label styleName="{style.inline}" ui:field="placeName"/></b>: </div> 
		<g:StackPanel ui:field="stackPanelApplications"/>
	</g:HTMLPanel>
</ui:UiBinder> 