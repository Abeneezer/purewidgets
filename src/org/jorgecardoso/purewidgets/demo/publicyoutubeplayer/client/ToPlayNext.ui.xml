<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
    ui:generateLocales="default" >

<ui:style src="css/ui.css" />
		
		<g:HorizontalPanel styleName='{style.searchresults}' >
			<g:cell >
				<g:VerticalPanel>
					 <g:HTMLPanel>
						<h2><ui:msg description="Search Results">Search Results</ui:msg></h2>
					</g:HTMLPanel>
					
					<g:HorizontalPanel>
						<g:VerticalPanel ui:field="verticalPanelLeft"/>
			
						<g:VerticalPanel ui:field="verticalPanelRight"/>
					</g:HorizontalPanel>
				</g:VerticalPanel>
			</g:cell>
			<g:cell width="30%">
				<g:VerticalPanel styleName='{style.queue}'>
					<g:HTMLPanel>
						<h2><ui:msg description="Queue">Queue</ui:msg></h2>
					</g:HTMLPanel>
					<g:VerticalPanel ui:field="verticalPanelQueue"/>
				</g:VerticalPanel>
			</g:cell>
		</g:HorizontalPanel>
	
</ui:UiBinder> 

