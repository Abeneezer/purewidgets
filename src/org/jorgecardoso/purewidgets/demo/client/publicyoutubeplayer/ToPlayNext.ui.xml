<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
		<g:HorizontalPanel styleName='searchresults' >
			<g:cell width="75%">
				<g:VerticalPanel>
					<g:HTMLPanel>
						<h2>To Play Next</h2>
					</g:HTMLPanel>
					
					<g:HorizontalPanel>
						<g:VerticalPanel ui:field="verticalPanelLeft"/>
			
						<g:VerticalPanel ui:field="verticalPanelRight"/>
					</g:HorizontalPanel>
				</g:VerticalPanel>
			</g:cell>
			<g:cell width="25%">
				<g:VerticalPanel styleName='queue'>
					<g:HTMLPanel>
						<h2>Queue</h2>
					</g:HTMLPanel>
					<g:VerticalPanel ui:field="verticalPanelQueue"/>
				</g:VerticalPanel>
			</g:cell>
		</g:HorizontalPanel>
	
</ui:UiBinder> 

